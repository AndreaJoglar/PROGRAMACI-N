package ejercicio1;

public class Tienda {

	private Producto [] productos;
	private int contProductos;
	
	/**
	 * 
	 */
	public Tienda() {
		productos = new Producto [10];
		contProductos = 0;
	}
	
	public void insertarProducto (Producto p) { //agregaci√≥n
		if (contProductos < productos.length) {
			productos[contProductos] = p;
			contProductos++;
		}
	}
	
	
}
